#include <iostream>
#include <iomanip>

using namespace std;

int main()

{

    int r;
    int c;

    cout << "How many rows?";
    cin >> r;
    cout << "How many columns?";
    cin >> c;
    
    for (int row = 1; row <= r; ++row) 
    {
        for (int col = 1; col <= c; ++col) 
        {
            cout << "* ";
        }
        cout << endl;
    }
    return 0;
    
}
